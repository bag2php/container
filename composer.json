{
    "name": "bag2/container",
    "description": "Library for meta programming.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.3",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.4",
        "phpunit/phpunit": "^9.4",
        "psr/http-factory": "^1.0"
    },
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {
            "Bag2\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bag2\\Container\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
